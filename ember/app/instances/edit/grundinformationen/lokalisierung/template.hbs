{{#camac-form instance=model.instance meta=model.meta as |form|}}
  {{#form.input 'bezeichnung'}}
    <h4>Bezeichnung und Lokalisierung des Vorhabens</h4>
  {{/form.input}}
  {{form.input 'projektnummer'}}
{{/camac-form}}

{{camac-property-selector
  readonly  = model.instance.identifier
  selected  = (hash
    points  = points.value
    parcels = parcels.value
    municipality = model.instance.location.name
  )
  on-submit = (perform saveLocation)
}}
