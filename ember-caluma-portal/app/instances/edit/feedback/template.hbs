<LoadingOrNotfound
  @loading={{this.editController.instanceTask.isRunning}}
  @hasPermission={{can "read feedback of instance" this.editController.instance.value}}
>
  {{#if this.feedback}}
    <BeAttachmentList @loading={{this.loading}} @attachments={{this.feedback}} />
  {{else}}
    <div class="uk-text-center">{{t "feedback.info"}}</div>
  {{/if}}
</LoadingOrNotfound>