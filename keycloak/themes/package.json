  },
  "author": "Christian Zosel <christian@zosel.ch>",
  "scripts": {
    "build-ur": "node-sass --include-path=node_modules/ src/main/resources/theme/kanton-uri/login/resources/scss/ -o src/main/resources/theme/kanton-uri/login/resources/css/",
    "watch-ur": "node-sass --include-path=node_modules/ -w src/main/resources/theme/kanton-uri/login/resources/scss/ -o src/main/resources/theme/kanton-uri/login/resources/css/",
    "build-gr": "node-sass --include-path=node_modules/ src/main/resources/theme/kanton-gr/scss/ -o src/main/resources/theme/kanton-gr/login/resources/css/ && cp src/main/resources/theme/kanton-gr/login/resources/css/*.css src/main/resources/theme/kanton-gr/account/resources/css/",
    "build-so": "node-sass --include-path=node_modules/ src/main/resources/theme/kanton-so/scss/ -o src/main/resources/theme/kanton-so/login/resources/css/ && cp src/main/resources/theme/kanton-so/login/resources/css/*.css src/main/resources/theme/kanton-so/account/resources/css/"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "node-sass": "9.0.0",
    "uikit": "^3.16.19"
  }
