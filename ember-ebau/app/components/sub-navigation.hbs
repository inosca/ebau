<ul class="uk-nav uk-nav-default">
  {{#each this.instanceResources.records as |ir|}}
    {{#if ir.link}}
      <UkSubnav::Item
        @href={{concat "/cases/" @instanceId "/" ir.link}}
        class={{ir.classField}}
      >
        {{ir.name}}
        {{#if (eq ir.link "communications")}}
          <Communication::UnreadMessageBadge
            @instanceId={{@instanceId}}
            class="uk-margin-small-left"
          />
        {{/if}}
      </UkSubnav::Item>
    {{/if}}
  {{/each}}
</ul>