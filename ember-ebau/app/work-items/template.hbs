<WorkItemListWrapper
  @order={{this.order}}
  @responsible={{this.responsible}}
  @type={{this.type}}
  @status={{this.status}}
  @role={{this.role}}
  @task={{this.task}}
  @username={{session-value "user.username"}}
  @application={{session-value "application"}}
  @serviceId={{session-value "service.id"}}
  @setFilter={{this.setFilter}}
  @useLinkTo={{true}}
  @baseRoute="cases.detail.work-items"
/>