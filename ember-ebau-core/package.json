{
  "name": "ember-ebau-core",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "//": [
    "html2canvas svg rendering of highlights seems to break in v1.0.0-rc7 and upwards",
    "potentially also related to missing transform-3d support, but details remain unclear"
  ],
  "dependencies": {
    "ember-apollo-client": "3.2.0",
    "ember-auto-import": "1.11.3",
    "ember-caluma": "8.0.0",
    "ember-cli-babel": "7.26.6",
    "ember-cli-htmlbars": "5.7.1",
    "ember-fetch": "8.1.1",
    "ember-intl": "5.7.0",
    "ember-leaflet": "5.0.0-beta.7",
    "ember-load-initializers": "2.1.2",
    "graphql": "15.5.3",
    "html2canvas": "1.3.2",
    "leaflet": "1.7.1",
    "proj4leaflet": "1.0.2"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.3.2",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.4.2",
    "@glimmer/component": "1.0.4",
    "@glimmer/tracking": "1.0.4",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.24.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-moment-shim": "3.8.0",
    "ember-cli-sass": "10.0.1",
    "ember-cli-showdown": "4.5.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-composable-helpers": "4.5.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-engines": "0.8.19",
    "ember-export-application-global": "2.0.1",
    "ember-math-helpers": "2.16.0",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-page-title": "6.2.2",
    "ember-pikaday": "3.0.0",
    "ember-power-select": "4.1.6",
    "ember-promise-helpers": "1.0.9",
    "ember-qunit": "5.1.4",
    "ember-resolver": "8.0.3",
    "ember-source": "3.24.5",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "3.3.0",
    "ember-try": "1.4.0",
    "ember-uikit": "4.0.0",
    "eslint": "7.25.0",
    "eslint-plugin-ember": "10.4.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "qunit": "2.17.1",
    "qunit-dom": "2.0.0",
    "sass": "1.40.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
