{
  "name": "ember-ebau-core",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "//": [
    "html2canvas svg rendering of highlights seems to break in v1.0.0-rc7 and upwards",
    "potentially also related to missing transform-3d support, but details remain unclear"
  ],
  "dependencies": {
    "@faker-js/faker": "7.5.0",
    "@fortawesome/ember-fontawesome": "0.4.0",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "@projectcaluma/ember-core": "11.0.0-beta.27",
    "@projectcaluma/ember-form": "11.0.0-beta.27",
    "ember-apollo-client": "4.0.2",
    "ember-auto-import": "2.4.2",
    "ember-can": "4.2.0",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.1.0",
    "ember-composable-helpers": "5.0.0",
    "ember-data": "3.28.10",
    "ember-data-resources": "4.0.0",
    "ember-fetch": "8.1.2",
    "ember-intl": "5.7.2",
    "ember-leaflet": "5.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-pikaday": "4.0.0",
    "ember-resources": "5.3.1",
    "ember-uikit": "6.0.0",
    "file-saver": "2.0.5",
    "filesize": "9.0.11",
    "graphql": "15.7.2",
    "html2canvas": "1.0.0-rc.6",
    "leaflet": "1.8.0",
    "luxon": "3.0.3",
    "proj4leaflet": "1.0.2"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.5.0",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.8.1",
    "@embroider/test-setup": "1.8.3",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.28.5",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.2",
    "ember-cli-sass": "11.0.1",
    "ember-cli-showdown": "6.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-page-title": "7.0.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.9",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "4.13.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "11.0.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.1",
    "loader.js": "4.7.0",
    "miragejs": "0.1.45",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "qunit": "2.19.1",
    "qunit-dom": "2.0.0",
    "sass": "1.54.8",
    "webpack": "5.74.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-pikaday"
  }
}
